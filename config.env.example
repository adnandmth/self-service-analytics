# Database Configuration
DATABASE_URL=postgresql://db_postgres:password@biops-postgres:5432/panoptes
POSTGRES_USER=db_postgres
POSTGRES_PASSWORD=your_secure_password

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4
OPENAI_MAX_TOKENS=2000
OPENAI_TEMPERATURE=0.1

# Application Configuration
SECRET_KEY=your_secret_key_here_change_this_in_production
DEBUG=false
LOG_LEVEL=INFO

# Security Configuration
JWT_SECRET_KEY=your_jwt_secret_key_here
JWT_ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Rate Limiting
RATE_LIMIT_PER_MINUTE=60

# Query Settings
MAX_QUERY_EXECUTION_TIME=300
MAX_RESULT_ROWS=10000

# Redis Configuration
REDIS_URL=redis://redis:6379

# CORS Settings
ALLOWED_ORIGINS=["http://localhost:3000", "http://localhost:8080", "http://your-domain.com"]
ALLOWED_HOSTS=["*"]

# Database Schema Access (comma-separated)
BI_SCHEMAS=["bi_reports", "dwh_aggregate", "dwh_metadata"]

# Read-only Tables (comma-separated)
READONLY_TABLES=[
    "bi_reports.users",
    "bi_reports.clients_backload",
    "bi_reports.performance_projects",
    "bi_reports.management_wly_backload",
    "dwh_aggregate.listings_bi",
    "dwh_aggregate.leads_angi",
    "dwh_metadata.ventures",
    "dwh_metadata.countries"
] 